<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.adeusunibratec.ha"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.com.adeusunibratec.mb.SplashActivity"
            android:label="@string/title_activity_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.com.adeusunibratec.mb.PrincipalActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="br.com.adeusunibratec.mb.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="br.com.adeusunibratec.mb.ResidenciasActivity"
            android:label="@string/title_activity_residencias" >
        </activity>
        <activity
            android:name="br.com.adeusunibratec.mb.OpcoesUsuariosActivity"
            android:label="@string/title_activity_opcoes_usuarios" >
        </activity>
        <activity
            android:name="br.com.adeusunibratec.mb.FavoritosActivity"
            android:label="@string/title_activity_favoritos" >
        </activity>
    </application>

</manifest>